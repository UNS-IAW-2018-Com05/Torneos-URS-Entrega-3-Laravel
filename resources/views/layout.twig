              {{ admin }} <span class="caret"></span>
          </a>
          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton">
            <a type="button" class="btn btn-primary" style="height: 100%; width: 100%;" href="/logout">Log out</a>
          </div>

        </div>
      </ul>
    </div>
